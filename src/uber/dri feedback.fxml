   <children>
      <Label layoutX="33.0" layoutY="25.0" text="Enter your feedback here">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextArea fx:id="feedarea" layoutX="33.0" layoutY="78.0" prefHeight="257.0" prefWidth="539.0" />
      <Button layoutX="494.0" layoutY="350.0" mnemonicParsing="false" onAction="#save" text="Confirm" />
      <Button layoutX="45.0" layoutY="350.0" mnemonicParsing="false" onAction="#close" text="Back" />
   </children>
</AnchorPane>
